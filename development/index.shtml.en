<script type="text/javascript">
//Loosely based on https://github.com/tuo/jenkins-monitor
var base_url = "https://mf4.xiph.org/jenkins/view/opus";
var jobs_to_be_excluded = ["baz"];

var jenkinsDashboard = {
	composeHtmlFragement: function(jobs){
		var fragment = document.createDocumentFragment();
		var fragment = "<div><ul>";
		$.each(jobs, function(){
			if(($.inArray(this.name, jobs_to_be_excluded) == -1)){
				if(this.color=="red"){
                                 fragment += ("<li class='CI CI" + this.color + "'><a href=" + this.url + " class='CI'>" + this.name + "</a><span class='CIcon'><a href='" + this.url +"lastBuild/console'><img src='terminal.png' class='CIcon' width=16 height=16 alt='[console]'/></a></span></li>");
                                } else {
                                 fragment += ("<li class='CI CI" + this.color + "'><a href=" + this.url + " class='CI'>" + this.name + "</a></li>");
                                }
			}
		});
		dashboardLastUpdatedTime = new Date();
		fragment +="</ul></div><br style='clear:both;' /><span class='CItime'>Updated: " + dashboardLastUpdatedTime.toString()  + "</span>";
		$("#CIcontent").html(fragment);
	},
	updateBuildStatus : function(data) {
		jenkinsDashboard.composeHtmlFragement(data.jobs);
	}
}

$(document).ready(function(){	
	ci_url = base_url + "/api/json";
	var counter = 0;
	var lastData = null;
	var monitor = function(){
	 counter++;
	 $.jsonp({
	        url: ci_url + "?format=json&jsonp=?",
	        dataType: "jsonp",
	        // callbackParameter: "jsonp",
	        timeout: 10000,
	        beforeSend: function(xhr) {
	        	if(counter == 1){
	        	        $("#CIcontent").html("<h2 id='loading'>Opus CI monitor loading...</h2>");
	        	};  	
	        },
	        success: function(data, status){
	        	jenkinsDashboard.updateBuildStatus(data);
	        },
	        error: function(XHR, textStatus, errorThrown){
	        	if($("#error_loading").length <= 0){
	        		$("#CIcontent").html('<h1 id="error_loading"> Connection failed, reloading Retrying........</h1>');
	        	}
	        }
	    });
	};
	monitor();
        var auto_refresh = setInterval(monitor, 30000);
})
</script>

